{
  "name": "@conduct/ecs",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build:compiler": "rm tsconfig.tsbuildinfo && npx tsc -p tsconfig.compiler.json && mv src/compiler.js src/compiler.cjs",
    "build": "tsc",
    "benchmark:node": "node dist/benchmark.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "ts-patch": "^3.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
